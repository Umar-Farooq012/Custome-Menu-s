SELECT 
    LEVEL,
   MENU_DESCRIPTION AS LABEL,
    DECODE(CONNECT_BY_ISLEAF, 0, '', 'f?p=&APP_ID.:' || TO_CHAR(PAGE_NUMBER) || ':&SESSION.:::::') AS TARGET,
    'YES' AS IS_CURRENT,
    NVL(ICON_IMAGE, 'fa-apex_square') AS IMAGE,
    PARENT_MENU_CODE
FROM 
    DEV_TABLE_MENU_REGISTRATION
START WITH 
    PARENT_MENU_CODE IS NULL
CONNECT BY 
    PRIOR MENU_CODE = PARENT_MENU_CODE
    ORDER SIBLINGS BY SERIAL_NUMBER;
